{
  "private": true,
  "name": "Portfolio",
  "description": "Portfolio for Elias Norrby, developer, musician & photographer",
  "author": "Elias Norrby <elias.norrby@gmail.com>",
  "license": "MIT",
  "starter-name": "gatsby-starter-portfolio-cara",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "develop:cypress": "cross-env CYPRESS_SUPPORT=y yarn develop",
    "build:cypress": "cross-env CYPRESS_SUPPORT=y yarn build",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "check-format": "prettier --list-different '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}'",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,html,vue,css,less,scss,graphql,yaml,yml,json,md,mdx}'",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 0",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@lekoarts/gatsby-theme-cara": "^1.0.11",
    "gatsby": "^3.14.1",
    "gatsby-plugin-google-analytics": "^2.1.8",
    "gatsby-plugin-manifest": "^2.2.6",
    "gatsby-plugin-netlify": "^2.1.6",
    "gatsby-plugin-offline": "^3.0.29",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-responsive": "^8.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@eliasnorrby/commitlint-config": "^2.2.0",
    "@eliasnorrby/prettier-config": "^4.2.30",
    "@eliasnorrby/semantic-release-config": "^3.0.23",
    "cross-env": "^7.0.0",
    "husky": "^4.2.1",
    "prettier": "^2.4.1",
    "semantic-release": "^17.2.3"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "version": "0.0.0-development",
  "repository": {
    "type": "git",
    "url": "https://github.com/eliasnorrby/portfolio-gatsby.git"
  }
}
